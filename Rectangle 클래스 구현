        "class Rectangle:\n",
        "  def __init__(self, width, height):\n",
        "    self.width=width\n",
        "    self.height=height\n",
        "  def area(self):\n",
        "    return self.width*self.height\n",
        "\n",
        "a=int(input(\"가로 입력: \"))\n",
        "b=int(input(\"세로 입력: \"))\n",
        "Rectangle=Rectangle(a,b)\n",
        "print(f\"넓이: {Rectangle.area()}\")\n"
      ]
